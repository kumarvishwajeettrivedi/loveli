{
  "name": "loveli",
  "version": "1.0.0",
  "description": "AI-powered anonymous chat platform - Meet strangers, share stories, stay safe with AI watching your back",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "ws": "node server.js",
    "deploy:vercel": "vercel --prod",
    "deploy:docker": "./scripts/deploy.sh docker",
    "deploy:production": "./scripts/deploy.sh production",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker logs -f",
    "validate": "npm run lint && npx tsc --noEmit && npm run build",
    "env:check": "node -e \"console.log('Environment check passed')\"",
    "type-check": "tsc --noEmit",
    "build:clean": "rm -rf .next && npm run build"
  },
  "dependencies": {
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "redis": "^4.6.8",
    "postgres": "^3.3.5",
    "openai": "^4.20.1",
    "@huggingface/inference": "^2.3.2",
    "uuid": "^9.0.0",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.29",
    "@types/node": "^20.5.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@types/uuid": "^9.0.2",
    "eslint": "^8.47.0",
    "eslint-config-next": "^14.0.0"
  },
  "keywords": ["chat", "anonymous", "ai", "moderation", "omegle", "nextjs"],
  "author": "Loveli Team",
  "license": "MIT"
}
